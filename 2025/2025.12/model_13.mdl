{UTF-8}
"acc shock in-flow"[Goal]=
	accidents[Goal]*accident severity
	~	
	~		|

shock effect on performance[Goal]=
	Accident shock level[Goal]*shock sensitivity
	~	
	~		|

accident rate B=
	1
	~	 [0,5,0.1]
	~		|

performance[A]=
	cAA*Resources[A] + cBA*Resources[B]+ (1-sw A to protective)* resource generative outcome\
		[A] + sw A to protective*shock effect on performance[A] ~~|
performance[B]=
	cAB*Resources[A] + cBB*Resources[B] + (1-sw B to protective)* resource generative outcome\
		[B] + sw B to protective*shock effect on performance[B]
	~	Dmnl
	~	add stochasticity
		potentially generative vs protective
		generative can be additive with a small stochastic component and for protective with \
		can have a rare event large impact (like safety)
		combined performance determines endogenous resource inflow of future
	|

Accident shock level[Goal]= INTEG (
	"acc shock in-flow"[Goal]-recovery rate[Goal],
		0)
	~	
	~		|

accidents[A]=
	RANDOM POISSON( 0 , 5 , accident rate A* TIME STEP, 0 , 1 , seed ) ~~|
accidents[B]=
	RANDOM POISSON( 0 , 5 , accident rate B* TIME STEP, 0 , 1 , 2*seed )
	~	
	~		|

recovery time[Goal]=
	MAX(0.1, recovery time base/(1+k asp*Resources[Goal]))
	~	Month
	~		|

recovery rate[Goal]=
	Accident shock level[Goal]/recovery time[Goal]
	~	
	~		|

gen stoch range=
	10
	~	 [0,50,0.1]
	~		|

resource generative outcome[A]=
	RANDOM NORMAL( -gen stoch range , gen stoch range , 0 , gstdv , seed ) ~~|
resource generative outcome[B]=
	RANDOM NORMAL( -gen stoch range , gen stoch range , 0 , gstdv , 2*seed )
	~	
	~		|

sw B to protective=
	0
	~	Dmnl [0,1,1]
	~		|

change in asp[Goal]=
	-Perceived perfromance gap[Goal]*sw endogen aspiration/t change asp
	~	1/Month
	~	there should be some nonlinearity here, if perf gap is zero, change in aspirations \
		should be positive rather than zero.
		
		Georgia: adding (1+k asp*Accident shock level), with big accidents, aspiration \
		accelerates. -> crisis reveal aspirations.
		“Aspirations adjust to the performance gap at a base rate determined by \
		t change asp, but this rate is multiplied by (1 + sensitivity × shock). \
		If shocks are present, managers shorten the time horizon for adjustment \
		— they react faster.”
	|

accident rate A=
	1
	~	1/Month [0,5,0.1]
	~		|

accident severity=
	10
	~	 [0,50,1]
	~	Magnitude of performance loss per accident event
	|

total investments=
	SUM(Resources[Goal!])
	~	
	~		|

k asp=
	0.02
	~	 [0.001,0.5]
	~	Sensitivity of aspiration updating to shocks.
		   Higher = aspirations move faster when accidents occur
	|

recovery time base=
	12
	~	Month [5,30,1]
	~		|

shock sensitivity=
	1
	~	Dmnl [0,50,1]
	~		|

sw A to protective=
	0
	~	Dmnl [0,1,1]
	~		|

aspiration[Goal]= INTEG (
	change in asp[Goal],
		exogen aspiration)
	~	Dmnl [0,1000,1]
	~	if perfromance is consistently falling short, adjust aspiration down
		if aspirations are consistently met, adjust them up
		
		future: market trends, ecosystem dynamics
	|

init perc comb perf= INITIAL(
	cAA*init A+cBA*init B
	+
	cAB*init A+cBB*init B)
	~	Dmnl
	~		|

Perceived comb perf= INTEG (
	change in perc comb perf,
		init perc comb perf)
	~	Dmnl
	~		|

Perceived perfromance gap[Goal]=
	SMOOTH( performance gap[Goal] , t update perf gap )
	~	Dmnl
	~		|

change in perc comb perf=
	(combined performance-Perceived comb perf)/IF THEN ELSE( combined performance>Perceived comb perf\
		 , t adj perc upwards , t adj perc downwards )
	~	1/Month
	~		|

t adj perc upwards=
	20
	~	Month [0.01,50,0.5]
	~		|

resource inflow=
	sw endog inflow*endogen resource inflow
	+
	(1-sw endog inflow)*exogen inflow
	~	Dollar/Month [0,100,0.1]
	~	endogenous inflow: function of performance (combined performance)
	|

endogen resource inflow=
	(1/sens res inflow) * LN(1+exp(sens res inflow*inflow per perf*Perceived comb perf))
	~	Dollar/Month
	~	https://www.desmos.com/calculator/dnhbwdyqdq
	|

seed=
	1
	~	 [1,5000,1]
	~		|

exogen aspiration=
	100
	~	Dmnl [0,1000,1]
	~	if perfromance is consistently falling short, adjust aspiration down
		if aspirations are consistently met, adjust them up
		
		future: market trends, ecosystem dynamics
	|

sens res inflow=
	1
	~	Dmnl [0,2,0.1]
	~		|

t change asp=
	40
	~	Month [1,50,1]
	~		|

gstdv=
	0
	~	Dmnl [0,30,0.5]
	~		|

inflow per perf=
	0.04
	~	Dollar/Month [0,0.5,0.001]
	~	0.04 seems to create long term equilibrium
	|

t adj perc downwards=
	2
	~	Month [0.01,50,0.5]
	~		|

t update perf gap=
	5
	~	Month [1,20,1]
	~		|

performance gap[Goal]=
	aspiration[Goal] - performance[Goal]
	~	Dmnl
	~		|

sw endog inflow=
	0
	~	Dmnl [0,1,1]
	~		|

sw endogen aspiration=
	0
	~	Dmnl [0,1,1]
	~		|

init resource[A]=
	init A ~~|
init resource[B]=
	init B
	~	Dollar [0,200,1]
	~		|

combined performance=
	SUM(performance[Goal!])
	~	Dmnl
	~		|

init res[Goal]= INITIAL(
	(1-eq init)*init resource[Goal])
	~	Dollar
	~		|

inv weight[Goal]=
	exp(softmax snstvty * performance gap[Goal]) / pg denum
	~	Dmnl
	~		|

Goal:
	A, B
	~	Dmnl
	~	lets move back to two goals (p,p), (g,g), (g,p)
	|

deterioration resources[Goal]=
	Resources[Goal]/t deteriorate
	~	Dollar/Month
	~		|

Resources[Goal]= INTEG (
	maturing inv[Goal]-deterioration resources[Goal],
		init res[Goal])
	~	Dollar
	~		|

init A=
	80
	~	Dollar [0,200,1]
	~		|

pg denum=
	SUM(exp(softmax snstvty * performance gap[Goal!]))
	~	Dmnl
	~	(exp(softmax snstvty * performance gap[A]) + exp(softmax snstvty * \
		performance gap[B]))
	|

init B=
	80
	~	Dollar [0,200,1]
	~		|

maturing inv[Goal]=
	Undeveloped Resources[Goal]/t mature
	~	Dollar/Month
	~		|

init UR[Goal]= INITIAL(
	(init res[Goal]/t deteriorate)*t mature)
	~	Dollar
	~		|

t mature=
	5
	~	Month [1,20,1]
	~		|

Undeveloped Resources[Goal]= INTEG (
	investing[Goal]-maturing inv[Goal],
		init UR[Goal])
	~	Dollar
	~		|

exogen inflow=
	8
	~	Dollar/Month [0,20,0.1]
	~		|

eq init=
	0
	~	Dmnl [0,1,1]
	~		|

softmax snstvty=
	0.03
	~	Dmnl [0,0.5,0.001]
	~		|

investing[Goal]=
	inv weight[Goal] * resource inflow
	~	Dollar/Month
	~		|

cAB=
	0
	~	1/Dollar [-1,1,0.01]
	~		|

cBA=
	0
	~	1/Dollar [-1,1,0.01]
	~		|

cAA=
	1
	~	1/Dollar [0,1,0.01]
	~		|

cBB=
	1
	~	1/Dollar [0,1,0.01]
	~		|

t deteriorate=
	25
	~	Month [1,50,1]
	~		|

